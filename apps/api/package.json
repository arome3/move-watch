{
  "name": "@movewatch/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--no-node-snapshot' tsx watch src/index.ts",
    "build": "tsc",
    "start": "node --no-node-snapshot dist/index.js",
    "clean": "rm -rf dist",
    "indexer": "NODE_OPTIONS='--no-node-snapshot' tsx src/workers/indexer.ts",
    "indexer:prod": "node --no-node-snapshot dist/workers/indexer.js",
    "action-worker": "NODE_OPTIONS='--no-node-snapshot' tsx src/workers/actionWorker.ts",
    "action-worker:prod": "node --no-node-snapshot dist/workers/actionWorker.js",
    "block-indexer": "NODE_OPTIONS='--no-node-snapshot' tsx src/workers/blockIndexer.ts",
    "block-indexer:prod": "node --no-node-snapshot dist/workers/blockIndexer.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aptos-labs/ts-sdk": "^1.26.0",
    "@movewatch/database": "workspace:*",
    "@movewatch/shared": "workspace:*",
    "@upstash/redis": "^1.28.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "isolated-vm": "^6.0.2",
    "jose": "^5.2.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.5.10",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0"
  }
}
